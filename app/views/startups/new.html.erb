<div class="row">
  <div class="large-4 columns">

    <button id="pull-from-angellist">Pull Info From Angellist</button>

    <%= form_for @startup do |f| %>

      <%= f.label "Angellist Id" %>
      <%= f.text_field :angellist_id %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :bio %>
      <%= f.text_field :bio %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :order %>
      <%= f.text_field :order %>

      <%= f.label :ceo_name %>
      <%= f.text_field :ceo_name %>

      <%= f.label :ceo_img_path %>
      <%= f.text_field :ceo_img_path %>

      <%= f.submit %>

    <% end %>

  </div>
</div>

</script>